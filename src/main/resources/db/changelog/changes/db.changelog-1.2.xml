<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet author="NCH" id="changelog-1.1">
         <dropColumn tableName="client" columnName="client_name"/>
         <dropColumn tableName="client" columnName="client_mail"/>
          <addColumn tableName="client" >
            <column name="client_name" type="varchar(50)"/>
        </addColumn>
         <addColumn tableName="client" >
            <column name="client_mail" type="varchar(50)"/>
        </addColumn>
    </changeSet>
</databaseChangeLog>